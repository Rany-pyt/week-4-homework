int blockSize = 10;
int time = 0;
int i = 0;
int j = 0;
 
void setup() {
 size(1000, 700);
 noStroke();
 background(0);
 
 
 translate(blockSize*8, blockSize*8);
 for (i = 0; i < letters.length; i++) {
 
   int xPos = 0;
   int yPos = 0;
   for (j = 0; j < letters[i].length; j++) {
     if (letters[i][j] == 1) {
       fill(xPos*yPos, xPos*4, yPos*4);
       rect(xPos, yPos, blockSize, blockSize);//2.png xiaoguo
       //rect(xPos, yPos, xPos,yPos);   //1.pngxiaoguo
     }
     xPos += blockSize;
     if (j % 4 == 3) {
       xPos = 0;
       yPos += blockSize;
       time=time+1;
     }
   }
   translate(55, 25);
 }
}
